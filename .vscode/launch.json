  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'minigrep'",
      "cargo": {
        "args": [
          "build",
          "--bin=minigrep",
          "--package=minigrep"
        ],
        "filter": {
          "name": "minigrep",
          "kind": "bin"
        }
      },
      "args": [
        "dreary",
        "poem.txt"
      ],
      "env": {
        "RUST_BACKTRACE": "1"
      },
      "cwd": "${workspaceFolder}/projects/minigrep"
    },
    {
